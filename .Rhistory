library(here)
library(tidyverse)
mv_data = read.csv(here("data", "mv_data.csv"))
mv_long = mv_data %>%
pivot_longer(cols = c(8:39), values_to = "tones", names_to = "sentence") %>%
separate(sentence, into = c("sentence","position", "words")) %>%
mutate(sentence_type = substr(sentence, start = 1, stop = 3))
# Exlpore the data
## How many different tones were there?
desc = mv_long %>%
group_by(tones, sentence_type, position, sex) %>%
summarize(n = n()) %>%
filter(n > 10)
desc = mv_long %>%
group_by(tones, sentence_type, position, SEX) %>%
summarize(n = n()) %>%
filter(n > 10)
desc %>%
ggplot(aes(x = tones, y = n, fill = SEX)) + geom_col(color = "black", position = "dodge") +
facet_grid(sentence_type~position)
desc %>%
ggplot(aes(x = tones, y = n, fill = SEX)) + geom_col(color = "black", position = "dodge") +
facet_grid(sentence_type~position) +
theme(axis.text.x = element_text(angle = 90, vjust = 0.5, hjust=1))
desc %>%
ggplot(aes(x = tones, y = n, fill = SEX)) + geom_col(color = "black", position = "dodge") +
facet_grid(sentence_type~position) +
theme(axis.text.x = element_text(angle = 90, vjust = 0.5, hjust=1)) + theme_minimal()
desc %>%
ggplot(aes(x = tones, y = n, fill = SEX)) + geom_col(color = "black", position = "dodge") +
facet_grid(sentence_type~position) +
theme_minimal() +
theme(axis.text.x = element_text(angle = 90, vjust = 0.5, hjust=1))
